{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [ 
        { 
           "name": "frontend",
           "links": [
                "backend"
           ],
           "essential": false,
           "image": "dillonkyle/frontend",
           "portMappings": [
                {
                    "containerPort":5000,
                    "hostPort":80
                }
           ],
           "memory": 500
        },
        {
            "name": "backend",
            "image": "dillonkyle/backend",
            "memory": 500,
            "essential": true,
            "portMappings": [
                {
                    "containerPort":8080,
                    "hostPort":8080
                }
           ]
        }
     ]     
}